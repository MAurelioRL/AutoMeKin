my_result := optimize(
  structure(
  file = 'tag.xyz'
  charge = carga
  )
  ts = true
  xyz_output = 'tag_opt.xyz'
  dft
)
hess := hessian(
  structure(
  file = 'tag_opt.xyz'
  charge = carga
  )
  save_normal_modes = 'freq_tag.molden'
  method = 'hessianmethod'
  dft
)
my_result := thermodynamics(
  structure(load = hess)
  load = hess
  temperature = temp_amk kelvin
)

